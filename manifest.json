{
    "manifest_version": 3,
    "name": "RedirectGuard",
    "description": "Redirects Scam websites To Their Proper Counterparts",
    "version": "1.0.1",
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "Background.js",
        "scripts": ["Background.js"]
    }
}
